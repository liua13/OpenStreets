<div id="login-wrapper" class="wrapper">
  	<h1>Login</h1>
  	<p class="notloggedin">You may only post if you are logged in</p>
  	<form id='login' id="login" method="POST" action="/loggedin">
	  	<div><input type='text' name='username' placeholder="Username"></div>
	  	<div><input type='password' name='password' placeholder="Password"></div>
	  	<% if (incorrectLogin!="") { %>
				<p class="alert alert-danger"><%= incorrectLogin %></p>
			<% } %>
	  	<input type="submit" value="submit">
	</form>
	<span>Don't have an account?
		<a onclick="switchAccount('signup')">Sign up here!</a>
	<span>
</div>